        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mr_hunter69.mynotes"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "API_URL": "https://mynotes-backend-uoai.onrender.com", 
      "eas": {
        "projectId": "876310b0-c607-4980-8e50-1cf2f2caa125"
      }
    }
  }
}
